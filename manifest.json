{
    "manifest_version": 3,
    "name": "Security Header Checker",
    "version": "1.0",
    "description": "Analyze website security headers and identify vulnerabilities",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "showmeplease.html",
        "default_title": "Check Security Headers"
    },
    "icons": {
        "16": "securityimg.png",
        "48": "securityimg.png",
        "128": "securityimg.png"
    }
}